extends page

block append page_vars
    -var menu='Challenges';
    if page.metadata.submenu
        -submenu = page.metadata.submenu

block append head
    title VOTS2025 Challenge | 
        = submenu

block submenu
    -var menu_items = { 'Home': '/vots2025/index.html', 'Program': '/vots2025/program.html', 'Submit': '/vots2025/participation.html', 'Leaderboards': '/vots2025/leaderboards.html', 'Dataset': '/vots2025/dataset.html', 'People': '/vots2025/people.html' };
    .navbar.navbar-default.navbar-static-top(role='navigation')
        .container
            .navbar-header
                a(class='navbar-brand' href='/vots2025/') VOTS2025
            ul(class='nav navbar-nav nav-collapse')
                each val, key in menu_items
                    if submenu == key
                        li.active
                            a(href=val)= key
                    else
                        li
                            a(href=val)= key
            div(class='navbar-text navbar-right')
              a(class='navbar-conjunction' href='https://iccv.thecvf.com/') ICCV2025, Honolulu, Hawaii 

    .clearer
